<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:AppTest"
              xmlns:ffimageloading="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"
             x:Class="AppTest.MainPage">

    <StackLayout>
        <ffimageloading:CachedImage Source="{Binding PNGImageUrl}" HeightRequest="200" />
        <ffimageloading:CachedImage Source="{Binding WEBPImageUrl}" HeightRequest="200" />
    </StackLayout>
</ContentPage>